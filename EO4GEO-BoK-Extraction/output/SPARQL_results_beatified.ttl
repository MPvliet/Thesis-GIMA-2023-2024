eo4geo:GS1-6 a obok:Concept,
        skos:Concept ;
    rdfs:label "[GS1-6] Open data" ;
    obok:conceptStatus "Completed" ;
    obok:hasRecommendedMaterial <file:/uploaded/generated/ReferenceHasNoURL>,
        <http://doi.org/10.1007/978-94-6265-261-3>,
        <https://doi.org/10.1080/10580530.2012.716740>,

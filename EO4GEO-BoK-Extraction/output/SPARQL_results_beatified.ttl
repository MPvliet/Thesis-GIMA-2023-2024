eo4geo:GS2-5 a obok:Concept,
        skos:Concept ;
    rdfs:label "[GS2-5] Geo-information value chain" ;
    obok:conceptStatus "Completed" ;
    obok:hasRecommendedMaterial <file:/uploaded/generated/ReferenceHasNoURL>,
        <http://doi.org/10.2902/1725-0463.2009.04.art12>,
        <http://doi.org/10.2902/1725-0463.2010.05.art10.>,

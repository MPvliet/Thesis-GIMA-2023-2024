    obok:hasSkill eo4geo:15aaa4c7-e3ec-4ecc-bd59-47edae9e1667,
        eo4geo:514b5214-bb4a-4fcf-b3f0-538324247eb9,
        eo4geo:7d84b388-2a63-4fbc-a1ea-f6e66ac2f254,

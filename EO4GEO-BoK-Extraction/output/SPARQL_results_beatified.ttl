eo4geo:PP1-1-2 a obok:Concept,
        skos:Concept ;
    rdfs:label "[PP1-1-2] Electromagnetic spectrum" ;
    obok:conceptStatus "Completed" ;
    obok:contributedBy <https://orcid.org/0000-0003-3875-7909> ;
    obok:hasRecommendedMaterial <file:/uploaded/generated/ReferenceHasNoURL> ;
    obok:hasSkill eo4geo:4767f08d-4779-491d-90e2-e2bea5ab2f45,
        eo4geo:fe5ceeb3-e08a-49ff-bb4b-b80a1af3cab8 ;

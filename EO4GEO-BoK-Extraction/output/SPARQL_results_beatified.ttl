eo4geo:PP2-3 a obok:Concept,
        skos:Concept ;
    rdfs:label "[PP2-3] Detecting microwaves" ;
    obok:conceptStatus "Completed" ;
    obok:contributedBy <https://orcid.org/0000-0002-3097-9278>,
        <https://scholar.google.de/citations?user=ZEaPq_4AAAAJ&hl=de>,
        <https://www.geographie.uni-jena.de/en/Schmullius.html>,
        <https://www.linkedin.com/in/cl%C3%A9mence-dubois-272b8a110/?originalSubdomain=de> ;

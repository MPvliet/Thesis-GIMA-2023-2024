eo4geo:TA13-6-5 a obok:Concept,
        skos:Concept ;
    rdfs:label "[TA13-6-5] Detect and monitor ships" ;
    obok:conceptStatus "New" ;
    obok:contributedBy <https://www.linkedin.com/in/veronika-krieger-a443b4150/?locale=de_DE>,

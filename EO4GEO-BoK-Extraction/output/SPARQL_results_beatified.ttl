eo4geo:AM11-2 a obok:Concept,
        skos:Concept ;
    rdfs:label "[AM11-2] Graph theoretic descriptive measures of networks" ;
    obok:conceptStatus "Completed (GI-N2K)" ;
    obok:contributedBy <https://research.utwente.nl/en/persons/rob-lemmens> ;

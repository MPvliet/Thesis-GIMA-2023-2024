        <https://ieeexplore.ieee.org/document/7056425>,
        <https://ieeexplore.ieee.org/stamp/stamp.jsp?arnumber=1323117> ;
    obok:hasSkill eo4geo:120321e0-8f45-4f72-b582-01dd6a2bdf8d,
        eo4geo:a9d8bc14-4aed-4dfa-aa9b-e8517a866f09,
        eo4geo:b156f1ae-8bcd-47f2-838f-7e97c08866eb ;

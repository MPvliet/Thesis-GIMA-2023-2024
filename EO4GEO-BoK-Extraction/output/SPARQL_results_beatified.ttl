    obok:hasSkill eo4geo:4de89d41-9957-47a4-bc4a-4ba5e69f276f,
        eo4geo:539651b0-b481-4201-8593-3f28cb1a5d93,
        eo4geo:5d211554-2d15-4e66-9981-6c67385e302f,

eo4geo:GS5-1b a obok:Concept,
        skos:Concept ;
    rdfs:label "[GS5-1b] Ethics in the geospatial information society" ;
    obok:conceptStatus "In progress (GI-N2K)" ;
    obok:hasRecommendedMaterial <https://doi.org/10.1559/152304095782540546> ;
    obok:hasSkill eo4geo:0b43ad3c-b777-4c45-883b-a8602b38f676,

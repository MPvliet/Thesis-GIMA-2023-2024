    obok:hasRecommendedMaterial <https://doi.org/10.1080/136588198241806>,
        <https://doi.org/10.1111/j.1749-8198.2009.00241.x> ;
    obok:hasSkill eo4geo:2ae7d219-48e2-4c80-8f38-269f2a018c1a,

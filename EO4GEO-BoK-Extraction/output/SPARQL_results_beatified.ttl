eo4geo:IP3-13 a obok:Concept,
        skos:Concept ;
    rdfs:label "[IP3-13] Visual interpretation" ;
    obok:conceptStatus "Completed" ;
    obok:contributedBy <http://www.igik.edu.pl/en/a/Agata-Hoscilo>,
        <https://research.utwente.nl/en/persons/mariana-belgiu>,

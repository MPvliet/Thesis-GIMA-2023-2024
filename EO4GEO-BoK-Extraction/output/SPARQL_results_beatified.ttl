    obok:conceptStatus "In progress (GI-N2K)" ;
    obok:hasRecommendedMaterial <https://www.learntechlib.org/p/84724/> ;
    obok:hasSkill eo4geo:19c9377e-43cb-4c33-9fae-31218453cbd5,
        eo4geo:d0c74a0f-b046-4bb9-b697-2e64eff6a0f3,

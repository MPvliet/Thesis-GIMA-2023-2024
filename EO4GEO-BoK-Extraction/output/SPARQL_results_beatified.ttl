eo4geo:PP1-1-10 a obok:Concept,
        skos:Concept ;
    rdfs:label "[PP1-1-10] Solar constant, solar insolation, daily insolation" ;
    obok:conceptStatus "Completed" ;
    obok:contributedBy <https://orcid.org/0000-0003-3875-7909> ;
    obok:hasRecommendedMaterial <file:/uploaded/generated/ReferenceHasNoURL> ;
    obok:hasSkill eo4geo:21563d34-a663-4d1b-98f6-3b0c1d2895bd,
        eo4geo:5a5208b9-4c72-4d33-b57f-cca0ec0b0031,
        eo4geo:987d95dc-dda4-49f6-a41f-0ce5c5fe664b,

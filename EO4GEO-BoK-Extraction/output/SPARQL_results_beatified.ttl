eo4geo:PP2-2-6-5 a obok:Concept,
        skos:Concept ;
    rdfs:label "[PP2-2-6-5] Single-scale & multi-scale roughness" ;
    obok:conceptStatus "Completed" ;
    obok:contributedBy <https://www.researchgate.net/profile/Anke_Fluhrer> ;

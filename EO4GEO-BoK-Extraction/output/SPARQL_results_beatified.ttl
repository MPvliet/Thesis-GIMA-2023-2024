eo4geo:CF6-3 a obok:Concept,
        skos:Concept ;
    rdfs:label "[CF6-3] Topological relationships" ;
    obok:conceptStatus "Completed (GI-N2K)" ;
    obok:contributedBy <https://research.utwente.nl/en/persons/rob-lemmens> ;
    obok:hasRecommendedMaterial <https://www.itc.nl/library/education/core-of-giscience/> ;
    obok:hasSkill eo4geo:2d88417e-8467-4bbd-a560-c2137af038ee,
